node {
    stage('Updates') {
        echo 'Updating'
        sshagent(credentials: ['SSH-EC2']) {
            sh "ssh -o StrictHostKeyChecking=no -l ec2-user 10.0.1.100 'sudo docker exec -it frontend git pull'"
            sh "ssh -o StrictHostKeyChecking=no -l ec2-user 10.0.2.100 'sudo docker exec -it frontend git pull'"
        }
    }    
}